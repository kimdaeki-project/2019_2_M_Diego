<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="couponMapper">
  
  	
  	<insert id="couponInsert" parameterType="coupon">
  	
  		insert into coupon(coup_num,id,name,discount,deadline,sales_value)
  			values(coupon_seq.nextval, #{id}, #{name}, #{discount}, #{deadline}, #{sales_value})
  	
  	</insert>
  	
  	<select id="counponSelect" parameterType="coupon" resultType="coupon">
  	
  		select * from coupon where coup_num = #{coup_num}
  		
  	</select>
  	
  	<select id="couponMyList" parameterType="coupon" resultType="coupon">
  	
  		select * from coupon where id=#{id} order by coup_num desc
  	
  	</select>
  	
  	<select id="couponList" resultType="coupon">
  		select * from coupon order by coup_num desc
  	</select>
  	
  	<delete id="couponDelete" parameterType="coupon">
  		delete coupon where coup_num = #{coup_num}
  	</delete>
  
  </mapper>