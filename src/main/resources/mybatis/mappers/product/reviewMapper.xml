  <!--리뷰 write  -->
  <insert id="reviewWrite" parameterType="ReviewVO">
   insert into review(id, pro_num, rev_num,star,name,help,rev_date) values(
    #{id},#{pro_num},rev_seq.nextval,#{star},#{name},#{help},sysdate
    )
   </insert>
  
