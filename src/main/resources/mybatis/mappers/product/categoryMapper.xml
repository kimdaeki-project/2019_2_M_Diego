<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="categoryMapper">
  	
  	<insert id="categoryUpdate" parameterType="CategoryVO">
  		insert into category(cat_name, cat_code, cat_ref) values(#{cat_name}, #{cat_code},#{cat_ref})
  	
  	</insert>
  	
  	
  	
  
  
  </mapper>